{
  "compilerOptions": {
    "target": "es5",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "baseUrl": ".",
    "paths": {
      "@common/*": ["src/components/common/*"],
      "@home/*": ["src/components/home/*"],
      "@gallery/*": ["src/components/gallery/*"],
      "@project/*": ["src/components/project/*"],
      "@session/*": ["src/components/session/*"],
      "@signup/*": ["src/components/signup/*"],
      "@login/*": ["src/components/login/*"],
      "@attendance/*": ["src/components/attendance/*"],
      "@archiving/*": ["src/components/archiving/*"],
      "@mypage/*": ["src/components/mypage/*"],
      "@pages/*": ["src/pages/*"],
      "@styles/*": ["src/styles/*"],
      "@@types/*": ["src/types/*"],
      "@utils/*": ["src/utils/*"],
      "@utils": ["src/utils"],
      "@image/*": ["public/image/*"]
    }
  },
  "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx", "mp4.d.ts"],
  "exclude": ["node_modules"]
}
